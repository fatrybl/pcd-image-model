{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Train Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pointcloud_evaluation/src/pcd_eval/model/train.py",
            "args": [
                "--data-dir",
                "/workspace/datasets/train",
                "--name",
                "test_run",
                "--batch-size",
                "4",
                "--workers",
                "4",
                "--clip-model",
                "ViT-L/14",
                "--config-path",
                "${workspaceFolder}/pointcloud_evaluation/src/pcd_eval/model/sst/sst_encoder_config_v2.py",
            ],
            "justMyCode": false
        },
        {
            "name": "Cache embeddings",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pointcloud_evaluation/src/pcd_eval/model/cache_embeddings.py",
            "args": [
                "--config-path",
                "${workspaceFolder}/pointcloud_evaluation/src/pcd_eval/model/sst/sst_encoder_config_v2.py",
                "--checkpoint",
                "${workspaceFolder}/lidar-clippin/8hbg3w89/checkpoints/last.ckpt",
                "--data-path",
                "${workspaceFolder}/datasets/test",
                "--batch-size",
                "4",
                "--dataset-name",
                "once"
            ],
            "justMyCode": false
        }
    ]
}